<head>
	<link rel="stylesheet" type="text/css" href="/style.css">
	<title>All Transactions Overview</title>
</head>

<body>

<ol>
<% @rows.each do |row| %>
	<li><%= row %></li>
<% end %>
</ol>

<table>
<thead>
	<tr>
		<th>Account</th>
		<th>Date</th>
		<th>Payee</th>
		<th>Category</th>
		<th>Outflow</th>
		<th>Inflow</th>
	</tr>
</thead>
<% @rows.each do |row| %>
	<tr>
		<% currentRow = row.split(",")
		currentRow.each do |x| %>
			<td> <%= x %> </td>
		<% end %>
	</tr>
<% end %>
</table>









<p>Enter a line to remove:</p>
<form action="/printcsv" method="post">
	<p><input type="text" name="row_to_remove"></p>
	<p><input type="submit"></p>
</form>

</body>